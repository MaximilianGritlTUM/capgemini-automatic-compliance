{
  "_version": "1.53.0",
  "sap.app": {
    "id": "capgeminiappws2025",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{Capgemini Compliance Check}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.1",
      "toolsId": "c51afef8-2a01-4fe6-ae75-0beda66caa08"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/Z_CONFIG_SRV_UI",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.114.10",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "capgeminiappws2025.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "capgeminiappws2025.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "pattern": "complianceReports",                
          "name": "ComplianceReport",        
          "target": "ComplianceReport"  
        },
        {
          "pattern": "reportDetail/{reportId}",                
          "name": "ComplianceReportDetail",        
          "target": "ComplianceReportDetail"
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Dashboard",
          "viewName": "Dashboard"
        },
        "ComplianceReport": {
          "viewName": "ComplianceReport",
          "viewLevel": 1
        },
        "ComplianceReportDetail": {
          "viewName": "ComplianceReportDetail",
          "viewLevel": 2
        }
      }
    },
    "rootView": {
      "viewName": "capgeminiappws2025.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}